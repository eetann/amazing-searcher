{
    "manifest_version": 2,
    "name": "AmazingSearcher",
    "version": "0.1.0",
    "description": "Add useful infomation at result of Google Search",
    "author": "eetann",
    "content_scripts": [
        {
            "matches": ["https://www.google.com/search?*"],
            "css": ["css/content.css"],
            "js": ["js/chunk-vendors.js", "js/content.js"]
        }
    ],
    "web_accessible_resources": ["imgs/*"]
}
